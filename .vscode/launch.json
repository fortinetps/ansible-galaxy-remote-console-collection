    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Terminal (integrated) - Test fortigate_remote_console Ansible module with purgedhcp command",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/plugins/modules/fortigate_remote_console.py",
            // "cwd": "${workspaceRoot}",
            "env": {"PYTHONPATH": "${workspaceRoot}"},
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/.vscode/fortigate_remote_console_args_purgedhcp.json"
            ]
        },
        {
            "name": "Python: Terminal (integrated) - Test fortigate_remote_console Ansible module with become option and cli command",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/plugins/modules/fortigate_remote_console.py",
            // "cwd": "${workspaceRoot}",
            "env": {"PYTHONPATH": "${workspaceRoot}"},
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "${workspaceFolder}/.vscode/fortigate_remote_console_args_become.json"
            ]
        },
        {
            "name": "Python: Terminal (integrated) - Test fortigate_remote_console Ansible module with factoryreset command",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/plugins/modules/fortigate_remote_console.py",
            // "cwd": "${workspaceRoot}",
            "env": {"PYTHONPATH": "${workspaceRoot}"},
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/.vscode/fortigate_remote_console_args_factoryreset.json"
            ]
        }
    ]
}